<script>
    //alert("top");
    window.addEventListener( "keydown", doKeyDown, false );
    function doKeyDown(e){
        //alert("keyCode: ["+e.keyCode+"]");
        if (e.keyCode == 39) {    // left arrow
            window.location = "/hello/";
        }
    }  
</script>
  
